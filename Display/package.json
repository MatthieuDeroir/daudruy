{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "craco": "^0.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react-start": "dotenv -e .env.production craco start",
    "electron-start": "electron .",
    "build": "react-scripts build",
    "start": "dotenv -e .env.development concurrently \"serve -s build\" \"wait-on http://localhost:2000 && npm run electron-start\"",
    "dev": "dotenv -e .env.development craco start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "electron": "^26.2.2",
    "electron-builder": "^24.6.4",
    "wait-on": "^7.0.1"
  },
  "main": "main.js"
}
